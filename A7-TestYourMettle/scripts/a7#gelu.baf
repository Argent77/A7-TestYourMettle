IF
  DifficultyGT(NORMAL)
  !GlobalTimerNotExpired("cast","LOCALS")
  Global("slayer","LOCALS",0)
  Global("Slayer1","GLOBAL",0)
  NumInPartyAliveGT(1)
  See(Player1)
  !StateCheck(Player1,CD_STATE_NOTVALID)
  CheckStat(Player1,0,SANCTUARY)
  CheckStat(Player1,0,PROTECTION_FROM_EVIL)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    SetGlobalTimer("cast","LOCALS",ONE_ROUND)
    SetGlobal("slayer","LOCALS",1)
    FaceObject(Player1)
    DisplayStringHead(Myself,@50015)  // Come forth, creature of darkness! Show me your power!
    ClearActions(Player1)
    ActionOverride(Player1,MakeUnselectable(2))
    ActionOverride(Player1,PlayDead(30))
    Wait(2)
    ForceSpellRES("a7#slr0",Myself)
    ForceSpellRES("a7#slr1",Player1)
    SetInterrupt(TRUE)
  RESPONSE #25
    SetGlobal("slayer","LOCALS",1)
    Continue()
END

